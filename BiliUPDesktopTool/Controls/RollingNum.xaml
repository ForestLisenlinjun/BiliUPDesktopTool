            <Label x:Name="num1" Content="1"  Height="50"  Width="28" FontSize="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,200,0,400" />
            <Label x:Name="num2" Content="2"  Height="50"  Width="28" FontSize="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,250,0,350" />
