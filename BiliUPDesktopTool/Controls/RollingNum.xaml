            <Label x:Name="num7" Content="7"  Height="50"  Width="28" FontSize="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,500,0,100" />
            <Label x:Name="num8" Content="8"  Height="50"  Width="28" FontSize="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,550,0,50" />
