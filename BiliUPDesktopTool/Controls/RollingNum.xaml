             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BiliUPDesktopTool"
             mc:Ignorable="d" Width="28" Height="50" Loaded="UserControl_Loaded" ScrollViewer.VerticalScrollBarVisibility="Disabled" ClipToBounds="True">
