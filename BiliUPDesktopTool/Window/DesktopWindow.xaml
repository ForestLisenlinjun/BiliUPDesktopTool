<Window x:Class="BiliUPDesktopTool.DesktopWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BiliUPDesktopTool"
        mc:Ignorable="d"
        Title="DesktopWindow" WindowStartupLocation="Manual" Icon="/BiliUPDesktopTool;component/icon.ico" ShowInTaskbar="False" ResizeMode="NoResize" Loaded="Window_Loaded" Background="{x:Null}" WindowStyle="None" AllowsTransparency="True">
    <Grid>
        <local:DataViewerPanel x:Name="DVP" Tag="DesktopWnd" Margin="{Binding DesktopWnd_Margin,Source={x:Static local:Skin.Instance},UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}"/>
    </Grid>
</Window>
